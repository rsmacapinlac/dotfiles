account ritchie@macapinlac.com
host smtp.gmail.com
port 587
from ritchie@macapinlac.com
user ritchie@macapinlac.com
passwordeval "pass ritchie@macapinlac.com/gmail"
auth on
tls on
tls_trust_file	/etc/ssl/certs/ca-certificates.crt
# logfile /home/ritchie/msmtp.log

